{
    "compilerOptions": {
        "noEmit": true,
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "lib": ["ES2022", "DOM", "DOM.Iterable"],
        "module": "preserve",
        "noUncheckedIndexedAccess": true,
        "esModuleInterop": true,
        "moduleDetection": "force",
        "isolatedModules": true
    }
}